{
    "title": "Scrape data from a web page",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
        "url": {
            "title": "URL of the page",
            "type": "string",
            "description": "The URL of website you want to get the data from.",
            "editor": "textfield",
            "prefill": "https://www.apify.com/"
        },
        "topmediai_api_key": {
            "title": "API key for https://www.topmediai.com/",
            "type": "string",
            "isSecret": true,
            "description": "API key for https://www.topmediai.com/",
            "editor": "textfield",
            "prefill": ""
        },
        "song_genre": {
            "title": "Song genre used to generate the song",
            "type": "string",
            "description": "Song genre used to generate the song",
            "editor": "textfield",
            "prefill": "Hip Hop"
        }

    },
    "required": ["url", "topmediai_api_key"]
}
